+++ test/chmanage/dao/MediaPlanOpenManagerTest.php
@@ -0,0 +1,18 @@
+<?php
+require_once dirname(__FILE__).'/../ChmanageTestBase.php';
+require_once CLASS_DIR.'/application/mediaplan/dao/MediaPlanOpenManager.php';
+
+class MediaPlanOpenManagerTest extends ChmanageTestBase {
+    /** @test */
+    public function test_convertUrlToScreenName() {
+        $this->assertSame(
+            MediaPlanOpenManager::convertUrlToScreenName('http://blog.livedoor.jp/testtest'),
+            'testtest');
+        $this->assertSame(
+            MediaPlanOpenManager::convertUrlToScreenName('http://testtest.blog12.fc2.com'),
+            'testtest');
+        $this->assertSame(
+            MediaPlanOpenManager::convertUrlToScreenName('http://testtest.com'),
+            'testtest_com');
+    }
+}
