+<?php
+
+/*
+ * デジタル開設 チャンネルツール申請画面 入力フォームのモデル
+ */
+
+
+class ChTool {
+
+    protected $params;
+    protected $isError = false;
+
+
+    /*
+     * 対象となるフォーム項目の配列
+     */
+    protected $elements = array(
+
+        'company_nickname' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+        'zip_code' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+        'address1' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+        'address2' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+        'phone_no' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+        'fax_no' => array(
+            'type' => 'input',
+//            'isRequire' => array( 'value' => true, ),
