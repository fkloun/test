+        $this->view->display('application/_base.tpl');
+        return $this;
+    }
+
+    /*
+     * モード別の処理
+     */
+    public function modeExecute( $mode ) {
+        $userInfo = $this->model->getUserInfo();
+
+        switch ( $mode ) {
+            // 編集モード
+            case EDIT_MODE:
+                break;
+
+            // 新規追加モード
+            case CREATE_MODE:
+                break;
+
+            // 削除モード
+            case DELETE_MODE:
+                break;
+
+            // 最終確認モード
+            case CHECK_MODE:
+                break;
+
+            // 編集追加時のモード (writedata)
+            case EDIT_NOW:
+
+                // バリデーション
+                try {
+                    $this->model->validate();
+
+                } catch ( Exception $e ) {
+                    // バリデーション失敗時は表示モードに戻す
+                    $this->modeExecute( DISPLAY_MODE );
+
+                    // POSTされた値をフォームに設定
+                    $this->assignParams($this->model->getParams(), array('model'));
+                    break;
+                }
+
+                // バリデーション済みの値をフォームに設定
+                $params = $this->model->getParams();
+                $this->assignParams($params);
+                $this->view->assign( 'publish_date_view',  $this->model->getPublishDate( $params['publish_date']) );
+                
+                $categoryId = $this->model->getParam( 'category_id' );
+                $this->view->assign( 'category_name', $this->model->getCategoryName( $categoryId ));
+
+                // 最終確認 -編集モードのとき
+                $this->bodyTpl   = 'application/ch_tool_displayonly.tpl';
+                $this->submitTpl = 'application/ch_tool_submit.tpl';
+                // 送信先はdb登録用スクリプト
+                $this->view->assign( 'ACT_URI', 'toolSubmit' );
+                // 表示するformを選択
+                $this->view->assign( 'mode', 'check' );
+
+                // 編集モードとしておく
+                $this->view->assign( 'mode_chk', EDIT_NOW );
+                break;
+
+            // 新規追加時のモード (writedb.php)
+            case EDIT_NEW:
+                break;
+
+            // 表示モード
+            case DISPLAY_MODE:
+            default:
+                $this->bodyTpl   = 'application/ch_tool_form.tpl';
+                $this->submitTpl = 'application/ch_tool_submit.tpl';
+
+                $this->assignOwnerInfo();
+                $this->assignParams($this->model->getParams(), array('model'));
+                $this->view->assign( 'mode', 'edit' );
+                $this->view->assign( 'company_nickname', $userInfo['company_name'] );
+                $this->view->assign( 'publish_master',   $this->model->getPublishDates() );
+                // 月額課金に設定がある場合、値を入れる
+                if ( $userInfo['monthly_charge_flag'] ) {
+                    $this->view->assign( 'monthly_price', $userInfo['monthly_price'] );
+                    // 1つ前の有料申し込みがあるなら、銀行入力の値を引いて設定する
+                    $banks = $this->model->getChToolLastAppliactionByOwnerId( $userInfo['id'] );
+                    $this->assignParams($banks, array('id'));
+                } else {
+                    $this->view->assign( 'monthly_price', '0' );
+                }
+                $this->view->assign( 'ACT_URI', $_SERVER['PHP_SELF'] );
+                $this->view->assign( 'category_master',    $this->model->getCategoryAll( true ) );
+                break;
+        }
+        $this->view->assign( 'is_free',             ($userInfo['free_charge_flag'])    ? true : false );
+        $this->view->assign( 'is_monthly_charge',   ($userInfo['monthly_charge_flag']) ? true : false );
+        return $this;
+    }
+    
+    private function assignParams($params, array $ignoreKeys=array()) {
+        if ( !(isset($params) && is_array($params)) ) {
+            return;
+        }
+        foreach ( $params as $key => $val ) {
+            if ( in_array($key, $ignoreKeys, true) ) {
+                continue;
+            }
+            $this->view->assign( $key, $val );
+        }
+    }
+
+    /**
+     *  オーナー情報があればアサインする
+     */
+    private function assignOwnerInfo() {
+        $userInfo = $this->model->getUserInfo();
+        $ownerInfo = ChannelEstablishDao::factory()->findByOwnerId($userInfo['id']);
+        if ($ownerInfo === null) {
+            return;
+        }
+        $this->view->assign('zip_code', $ownerInfo['zip_code']);
+        $this->view->assign('address1', $ownerInfo['address1']);
+        $this->view->assign('address2', $ownerInfo['address2']);
+        $this->view->assign('phone_no', $ownerInfo['phone_no']);
+        $this->view->assign('fax_no',   $ownerInfo['fax_no']);
+
+        $this->view->assign('bank_id',   $ownerInfo['bank_id']);
+        $this->view->assign('bank_name', $ownerInfo['bank_name']);
+        $this->view->assign('bank_kana', $ownerInfo['bank_kana']);
+        
+        $this->view->assign('branch_code', $ownerInfo['branch_code']);
+        $this->view->assign('branch_name', $ownerInfo['branch_name']);
+        $this->view->assign('branch_kana', $ownerInfo['branch_kana']);
+        
+        $this->view->assign('account_type', $ownerInfo['account_type']);
+        $this->view->assign('account_no',   $ownerInfo['account_no']);
+        $this->view->assign('account_name', $ownerInfo['account_name']);
+        $this->view->assign('account_kana', $ownerInfo['account_kana']);
+    }
+
+
+    public function checkMode( $mode ) {
+        if( !isset($mode) || !in_array($mode, $this->getCheckModeList() ) ){
+            // モード無し時は入力モードでの処理
+            $this->mode = DISPLAY_MODE;
+        } else {
+            $this->mode = $mode;
+        }
+        return $this;
+    }
+
+    public function getMode() {
+        return $this->mode;
+    }
+
+    public function getCheckModeList() {
+        return $this->checkModeList;
+    }
 
 }
