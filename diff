+<?php
+require dirname(__FILE__).'/MediaPlanApplicationEntity.php';
+require dirname(__FILE__).'/dao/MediaPlanApplicationDao.php';
+/**
+ * メディアプラン申請データのレポジトリ。
+ * @author manato_kato
+ */
+class MediaPlanApplicationRepository {
+    private $dao;
+    public function __construct(MediaPlanApplicationDao $dao = null) {
+        $this->dao = $dao === null ? new MediaPlanApplicationDao() : $dao;
+    }
+    
+    public static function factory(MediaPlanApplicationDao $dao = null) {
+        return new self($dao);
+    }
+    
+    /**
+     * ownerIdと紐づくエンティティを取得する。
+     * @return MediaPlanApplicationEntity
+     */
+    public function getByOwnerId($ownerId) {
+        $params = $this->dao->findByOwnerId($ownerId);
+        return new MediaPlanApplicationEntity($params ? $params : null);
+    }
+    
+    /**
+     * エンティティを保存する。
+     * @param MediaPlanApplicationEntity $entity
+     * @return boolean storeの成否。既にAPPLYINGのデータが存在する場合は失敗する。
+     */
+    public function store(MediaPlanApplicationEntity $entity, array $userInfo, $ipAddress) {
+        $params = $entity->getParams();
+        $params[':owner_id']   = $userInfo['id'];
+        $params[':owner_mail'] = $userInfo['mail'];
+        $params[':ip_address'] = $ipAddress;
+        return $this->dao->insert($params);
+    }
+}
