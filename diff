+        $mode = $this->getMode();
+
+        // モード別の処理を呼び出す
+        $this->modeExecute( $mode );
+//        $this->view->assign('page_status', $page_status);
+//        $this->view->assign('error', $error);
+        $this->view->assign('user_info', $this->model->getUserInfo());
