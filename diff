diff --git payment/pay/common/class/application/mediaplan/controller/MediaPlanControllerBase.php payment/pay/common/class/application/mediaplan/controller/MediaPlanControllerBase.php
new file mode 100755
index 0000000..96531d5
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/controller/MediaPlanControllerBase.php
@@ -0,0 +1,77 @@
+<?php
+require_once CLASS_DIR.'/HtmlView.class.php';
+require_once CLASS_DIR.'/Controller.class.php';
+
+/**
+ * メディアプラン用のコントローラ基底クラス
+ * @author manato_kato
+*/
+abstract class MediaPlanControllerBase extends Controller {
+    protected $view;
+    protected $userInfo;
+
+    protected $bodyTpl;
+    
+    protected $get;
+    protected $post;
+    protected $cookie;
+    protected $server;
+
+    public function __construct() {
+        $this->setView(new HtmlView());
+    }
+    
+    public function setParams(array $params) {
+        $this->get    = isset($params['get'])    ? $params['get'] : array();
+        $this->post   = isset($params['post'])   ? $params['post'] : array();
+        $this->cookie = isset($params['cookie']) ? $params['cookie'] : array();
+        $this->server = isset($params['server']) ? $params['server'] : array();
+    }
+
+    protected abstract function executeImpl();
+    public function execute() {
+        $tokenKey = date('Y-m-d H-i-s');
+        setcookie("mediaplan_application_time", $tokenKey);
+        
+        $this->view = new HtmlView();
+        $this->executeImpl();
+        $this->view->assign('user_info',  $this->userInfo);
+        $this->view->assign('body_tpl',   $this->bodyTpl );
+        $this->view->assign('token',      $this->createToken($tokenKey) );
+        $this->view->display('application/mediaplan/_base.tpl');
+    }
+    
+    /**
+     * POSTされたTokenの値と、Cookieにセットされたキーの値の整合をチェックし、整合がとれなければ例外を投げる。
+     */
+    protected function checkToken() {
+        if (isset($this->cookie['mediaplan_application_time']) && isset($this->post['token'])) {
+            if ($this->createToken($this->cookie['mediaplan_application_time']) === $this->post['token']) {
+                return;
+            }
+        }
+        throw new Exception("不正なトークンによるアクセス");
+    }
+    
+    protected function createToken($key) {
+        return sha1($key.MEDIAPLAN_ENTRY_SALT);
+    }
+    
+    public function setView(HtmlView $view) {
+        $this->view = $view;
+    }
+    
+    public function getView(HtmlView $view) {
+        return $this->view;
+    }
+
+    public function setUserInfo( $userInfo ) {
+        $this->userInfo = $userInfo;
+    }
+    
+    protected function assignAll(array $params) {
+        foreach ( $params as $name => $val ) {
+            $this->view->assign( $name, $val );
+        }
+    }
+}
\ No newline at end of file
