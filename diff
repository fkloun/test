+    
+    /**
+     * ownerIdに紐づく開設者情報を取得。
+     * とりあえず、establishsearchと同じSQL。
+     * @author manato_kato
+     * @param int $ownerId
+     */
+    public function getDescription($ownerId) {
+        $sql = <<<QUERY
+            SELECT description FROM channeltooluser
+                WHERE owner_id = :owner_id AND user_id = :owner_id LIMIT 1
+QUERY;
+        $sth = $this->prepare($sql);
+        $sth->execute( array(':owner_id' => $ownerId) );
+        $result = $sth->fetch(PDO::FETCH_ASSOC);
+        return isset($result['description']) ? $result['description'] : null;
+    }
 
