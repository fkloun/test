diff --git test/chmanage/ChmanageTestBase.php test/chmanage/ChmanageTestBase.php
new file mode 100755
index 0000000..e68d2ae
--- /dev/null
+++ test/chmanage/ChmanageTestBase.php
@@ -0,0 +1,23 @@
+<?php
+require_once dirname(__FILE__).'/../../chmanage/pay/common/conf/main_conf.php';
+require_once(dirname(__FILE__) . '/../../chmanage/pay/common/conf/paymenttool_conf.php');
+/**
+ * MediaPlanChmanageで使うテストの基底となるクラス。
+ * @author manato_kato
+ *
+ */
+abstract class ChmanageTestBase extends PHPUnit_Framework_TestCase {
+    /**
+     * テスト実行前のエラーレポートレベル
+     * @var integer
+     */
+    private static $defaultErrorReportingValue;
+    static function setUpBeforeClass() {
+        self::$defaultErrorReportingValue = ini_get('error_reporting');
+        ini_set('error_reporting', E_ALL & ~E_STRICT);
+    }
+
+    static function tearDownAfterClass() {
+        ini_set('error_reporting', self::$defaultErrorReportingValue);
+    }
+}
