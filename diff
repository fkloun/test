diff --git chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationController.php chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationController.php
new file mode 100755
index 0000000..ad30159
--- /dev/null
+++ chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationController.php
@@ -0,0 +1,54 @@
+<?php
+require_once dirname(__FILE__) . '/../AbstractApplicationPage.php';
+require_once dirname(__FILE__) . '/dao/MediaPlanApplicationDao.php';
+
+/**
+ * メディアプラン申請(Application)のコントローラの共通基底クラス
+ * @author manato_kato
+ */
+abstract class MediaPlanApplicationController extends AbstractApplicationPage {
+    protected $params;
+    public final function constructPage($smarty) {
+        $this->params = array_merge($_GET, $_POST);
+        $this->executeImpl($smarty);
+    }
+    
+    protected abstract function executeImpl($smarty);
+
+    /**
+     * パラメータからinvestigationを取得する
+     * @return string
+     */
+    protected function getInvestigation() {
+        switch ($this->getParam('investigation')) {
+            case 'OK':
+                return 'OK';
+        }
+        return 'NG';
+    }
+    
+    /**
+     * パラメータのidの値を用いてApplicationのEntityの取得を行う。
+     * @return array|null ApplicationEntityが取得できたらそれを表す連想配列、そうでなければnull
+     */
+    protected function getApplicationEntity() {
+        if (isset($this->params['id'])) {
+            $id = intval($this->params['id']);
+            $dao = new MediaPlanApplicationDao();
+            $app = $dao->findById($id);
+            if ($app !== false) {
+                return $app;
+            }
+        }
+        return null;
+    }
+    
+    /**
+     * $keyをインデックスに持つパラメータがあればそれを返し、なければ空文字を返す。
+     * @param string $key
+     * @return string
+     */
+    public function getParam($key) {
+        return isset($this->params[$key]) ? $this->params[$key] : '';
+    }
+}
\ No newline at end of file
