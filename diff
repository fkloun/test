diff --git payment/pay/common/class/application/mediaplan/controller/MediaPlanEntryController.php payment/pay/common/class/application/mediaplan/controller/MediaPlanEntryController.php
new file mode 100755
index 0000000..9b9f083
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/controller/MediaPlanEntryController.php
@@ -0,0 +1,75 @@
+<?php
+session_cache_limiter();
+require_once dirname(__FILE__).'/MediaPlanControllerBase.php';
+require_once dirname(__FILE__).'/../model/MediaPlanApplicationRepository.php';
+require_once dirname(__FILE__).'/../model/MediaPlanApplicationValidator.php';
+require_once dirname(__FILE__).'/../../dao/CommunityDao.php';
+
+class MediaPlanEntryController extends MediaPlanControllerBase {
+    // modeとして取りうる値
+    const PAGE_CONFIRM = 'confirm';
+    const PAGE_EDIT = 'edit';
+
+    public static function factory() {
+        return new self();
+    }
+
+    private function getNextPage($param) {
+        $modeList = array(
+            self::PAGE_CONFIRM,
+            self::PAGE_EDIT,
+        );
+        if ( isset($param['nextPage']) && in_array($param['nextPage'], $modeList ) ) {
+            return $param['nextPage'];
+        }
+        return self::PAGE_EDIT;
+    }
+    
+    protected function executeImpl() {
+        $ownChannel = CommunityDao::factory()->findOwnChannel($this->userInfo['id']);
+        $entity = MediaPlanApplicationRepository::factory()->getByOwnerId($this->userInfo['id']);
+        // APPLYING もしくは既にチャンネルを持ってい たら審査中ページにリダイレクト
+        if ( $entity->isApplying() || $ownChannel) {
+            $this->view->redirect( '/application/mediaplan/gate' );
+            return;
+        }
+        $this->clearAllCookies();
+        $this->executePage( $this->getNextPage( array_merge($this->post, $this->get) ) );
+    }
+
+    /**
+     * モード別の処理
+     */
+    private function executePage( $nextPage ) {
+        $validator = MediaPlanApplicationValidator::factory($this->post, $this->userInfo);
+        $this->assignAll( $validator->getParams() );
+        
+        switch ( $nextPage ) {
+        case self::PAGE_EDIT:
+            $this->executeEditPage();
+            break;
+        case self::PAGE_CONFIRM:
+            $this->executeConfirmPage($validator);
+            break;
+        }
+        $this->view->assign('user_info', $this->userInfo);
+        $this->view->assign('local_javascript_path', '/js/application/mediaplan_entry.js');
+    }
+    
+    private function executeEditPage() {
+        $this->bodyTpl = 'application/mediaplan/entry_form.tpl';
+        $this->view->assign( 'ACT_URI', $this->server['PHP_SELF'] );
+    }
+
+    private function executeConfirmPage($validator) {
+        // バリデーション失敗時は編集モードでエラーメッセージを表示する
+        if ($validator->hasError()) {
+            $this->executeEditPage();
+            $this->view->assign( 'errormsg', $validator->getErrorMessages() );
+            return;
+        }
+        
+        $this->bodyTpl = 'application/mediaplan/entry_confirm.tpl';
+        $this->view->assign( 'ACT_URI', 'entry_submit' );
+    }
+}
diff --git payment/pay/common/class/application/mediaplan/controller/MediaPlanEntrySubmitController.php payment/pay/common/class/application/mediaplan/controller/MediaPlanEntrySubmitController.php
new file mode 100755
index 0000000..49e58ed
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/controller/MediaPlanEntrySubmitController.php
@@ -0,0 +1,59 @@
+<?php
+require_once dirname(__FILE__).'/MediaPlanControllerBase.php';
+require_once dirname(__FILE__).'/../model/MediaPlanApplicationRepository.php';
+require_once dirname(__FILE__).'/../model/MediaPlanApplicationValidator.php';
+require_once dirname(__FILE__).'/../model/MediaPlanMailSender.php';
+require_once dirname(__FILE__).'/../../dao/CommunityDao.php';
+
+
+class MediaPlanEntrySubmitController extends MediaPlanControllerBase {
+    const ML_MAIL_TPL   = 'application/mediaplan/mail/entry_mail.tpl';
+    const USER_MAIL_TPL = 'application/mediaplan/mail/entry_mail_user.tpl';
+    
+    public static function factory() {
+        return new self();
+    }
+
+    public function executeImpl() {
+        $this->checkToken();
+        
+        $ownChannel = CommunityDao::factory()->findOwnChannel($this->userInfo['id']);
+        $entity = MediaPlanApplicationRepository::factory()->getByOwnerId($this->userInfo['id']);
+        // APPLYING もしくは既にチャンネルを持ってい たら審査中ページにリダイレクト
+        if ( $entity->isApplying() || $ownChannel) {
+            $this->view->redirect( '/application/mediaplan/gate' );
+            return;
+        }
+        
+        // postされてきた値をバリデーション
+        $validator = MediaPlanApplicationValidator::factory($this->post, $this->userInfo);
+        if ($validator->hasError()) {
+            $this->view->redirect('/application/info/error?message=validate');
+            return;
+        }
+        $params = $validator->getParams();
+        try {
+            $success = MediaPlanApplicationRepository::factory()
+                ->store( new MediaPlanApplicationEntity($params), $this->userInfo, $this->server['REMOTE_ADDR'] );
+            if( $success ) {
+                // nicoch-setup に送信
+                MediaPlanMailSender::factory(self::ML_MAIL_TPL)->sendMail(
+                    $this->userInfo['mail'],
+                    MEDIAPLAN_ENTRY_ARRIVE_MAIL_TO,
+                    $params
+                );
+                // 申込ユーザ に送信
+                MediaPlanMailSender::factory(self::USER_MAIL_TPL)->sendMail(
+                    'no-reply@dwango.co.jp',
+                    $this->userInfo['mail'],
+                    $params
+                );
+            }
+            // 完了画面に転送
+            $this->view->redirect( '/application/mediaplan/entry_complete' );
+        } catch (Exception $ex) {
+            $this->view->redirect('/application/info/error?message=db');
+        }
+    }
+
+}
