+                // バリデーション済みの値をフォームに設定
+                $this->assignParams( $this->model->getParams() );
+                // 最終確認 -編集モードのとき
+                $this->bodyTpl   = 'application/ch_entry_displayonly.tpl';
+                $this->submitTpl = 'application/ch_entry_submit.tpl';
+                // 送信先はdb登録用スクリプト
+                $this->view->assign( 'ACT_URI', 'entrySubmit' );
+                // 表示するformを選択
+                $this->view->assign( 'mode', 'check' );
+                $this->view->assign('user_info', $this->model->getUserInfo());
+                // 編集モードとしておく
+                $this->view->assign( 'mode_chk', EDIT_NOW );
+                break;
+
+                // 新規追加時のモード (writedb.php)
+            case EDIT_NEW:
+                break;
+
+            // 表示モード(デフォルトも)
+            case DISPLAY_MODE:
+            default:
+                $this->assignOwnerInfo();
+                $this->assignParams( $this->model->getParams() );
+
+                $this->bodyTpl   = 'application/ch_entry_form.tpl';
+                $this->submitTpl = 'application/ch_entry_submit.tpl';
+
+                $this->view->assign('user_info', $this->model->getUserInfo());
+                $this->view->assign( 'ACT_URI', $_SERVER['PHP_SELF'] );
+                $this->view->assign( 'mode',       'edit' );
+                break;
+        }
+
+        if(array_key_exists('blog_only' , $_GET)){
+            $isBlogOnly = 'true';
+        } else if(array_key_exists('is_blog_only' , $_POST)){
+            $isBlogOnly = $_POST['is_blog_only'];
+        } else {
+            $isBlogOnly = '';
+        }
+        
+        $this->view->assign('is_blog_only', $isBlogOnly );
+        // 共通の処理
+        $this->view->assign('local_javascript_path', '/js/application/ch_entry.js');
+        return $this;
+    }
+
+    private function assignParams($params, array $ignoreKeys=array()) {
+        if ( !(isset($params) && is_array($params)) ) {
+            return;
+        }
+        foreach ( $params as $key => $val ) {
+            if ( in_array($key, $ignoreKeys) ) {
+                continue;
+            }
+            $this->view->assign( $key, $val );
+        }
+    }
+
+    /**
+     *  オーナー情報があればアサインする
+     */
+    private function assignOwnerInfo() {
+        $userInfo = $this->model->getUserInfo();
+        $ownerInfo = ChannelEstablishDao::factory()->findByOwnerId($userInfo['id']);
+        if ($ownerInfo === null) {
+            return;
+        }
+        $this->view->assign('company_name', $ownerInfo['company_name']);
+        $this->view->assign('division_name', $ownerInfo['division_name']);
+        $this->view->assign('person', $ownerInfo['person']);
+    }
+
+    public function checkMode( $mode ) {
+        if( !isset($mode) || !in_array($mode, $this->getCheckModeList() ) ){
+            $this->mode = DISPLAY_MODE;
+        } else {
+            $this->mode = $mode;
+        }
+        return $this;
+    }
+
+    public function getMode() {
+        return $this->mode;
+    }
+
+    public function getCheckModeList() {
+        return $this->checkModeList;
+    }
 
 }
