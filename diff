diff --git payment/pay/common/class/application/mediaplan/model/MediaPlanMailSender.php payment/pay/common/class/application/mediaplan/model/MediaPlanMailSender.php
new file mode 100755
index 0000000..23f30b3
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/model/MediaPlanMailSender.php
@@ -0,0 +1,35 @@
+<?php
+/**
+ * メールを送ってくれるさん
+ * @author manato_kato
+ */
+class MediaPlanMailSender {
+    private $smarty;
+    private $tplPath;
+    
+    public function __construct($tplPath) {
+        $this->smarty = new Smarty();
+        $this->smarty->template_dir = constant('TPL_DIR');
+        $this->smarty->compile_dir  = constant('TPL_COMPDIR');
+        $this->tplPath = $tplPath;
+    }
+    
+    /**
+     * メールを送信する。
+     * @param string $fromMail 送信先メールアドレス
+     * @param string $toMail   送信元メールアドレス
+     * @param array  $params   テンプレートに割り当てるパラメータ
+     */
+    public function sendMail($fromMail, $toMail, array $params) {
+        $this->smarty->assign( 'from_mail', $fromMail );
+        $this->smarty->assign( 'to_mail'  , $toMail );
+        $this->smarty->assign( 'params'   , $params );
+        
+        $mailBody = $this->smarty->fetch($this->tplPath);
+        util::sendmail($mailBody);
+    }
+    
+    public static function factory($tplPath) {
+        return new self($tplPath);
+    }
+}
\ No newline at end of file
diff --git payment/pay/common/class/application/mediaplan/model/MediaPlanValidatorBase.php payment/pay/common/class/application/mediaplan/model/MediaPlanValidatorBase.php
new file mode 100755
index 0000000..b5cfd08
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/model/MediaPlanValidatorBase.php
@@ -0,0 +1,134 @@
+<?php
+/**
+ * パラメータのバリデーション・エラーメッセージの取得を扱うクラスの既定
+ * @author manato_kato
+ */
+abstract class MediaPlanValidatorBase {
+    protected $params;
+    protected $userInfo;
+    protected $errorMessages;
+    
+    // 派生クラスで上書きをする。
+    protected $elements = array();
+    
+    /**
+     * err_msg を指定しなかった場合のデフォルトのエラーメッセージ
+    */
+    protected $defaultErrorMsg = array( 'isRequire' => '入力してください。' );
+    
+    public function __construct( array $params, array $userInfo ) {
+        $this->setParams( $params );
+        $this->setUserInfo( $userInfo );
+    }
+
+    public function setParams( array $params ) {
+        $this->params = $params;
+    }
+    
+    public function getParams() {
+        return $this->filterByElements( $this->params );
+    }
+    
+    public function getUserInfo() {
+        return $this->userInfo;
+    }
+    
+    public function setUserInfo( array $userInfo ) {
+        $this->userInfo = $userInfo;
+    }
+    
+    public function getElements() {
+        return $this->elements;
+    }
+    
+    public function getElement( $name ) {
+        if( isset( $this->elements[$name] ) ) {
+            return $this->elements[$name];
+        }
+        return null;
+    }
+    
+    public function getElementNames() {
+        return array_keys($this->elements);
+    }
+    
+    private function filterByElements( array $params ) {
+        $result = array();
+        foreach( $params as $name => $val ) {
+            if( isset( $this->elements[$name] ) ) {
+                $result[$name] = (string) $val;
+            }
+        }
+        return $result;
+    }
+
+    /*
+     * エラーが有るかチェックし、ある場合はerrorMessageに追加する。
+    */
+    public function hasError() {
+        $this->errorMessages = array();
+        foreach( $this->elements as $name => $val ) {
+            $isRequire =  isset($val['isRequire']['value']) && $val['isRequire']['value'] === true;
+
+            $str = isset($this->params[$name]) ? $this->params[$name] : null;
+            $isEmpty = $str === null || trim($str) === '';
+            if( !$isRequire && $isEmpty ) {
+                // 入力必須でない項目が空なら、バリデーションの対象としない
+            } else if ( !is_string($str) ) {
+                // 文字列ではない
+                $this->setErrorMessage( $name, 'invalid', $val );
+            } else if ( $isRequire && $isEmpty ) {
+                // 入力必須のチェック
+                $this->setErrorMessage( $name, 'isRequire', $val );
+            } else {
+                $this->checkTypeError($str, $name, $val);
+            }
+        }
+        return !empty($this->errorMessages);
+    }
+    
+    private function checkTypeError($str, $name, $val) {
+        switch ( $val['type']) {
+            case 'text':
+            case 'radio':
+                break;
+            case 'input':
+                // チャンネル名だったら有効性チェック
+                if( isset($val['isChannelName']['value']) && $val['isChannelName']['value'] === true ) {
+                    if( !util::checkChannelName( $str ) ) {
+                        $this->setErrorMessage( $name, 'isChannelName', $val );
+                    }
+                }
+                break;
+            case 'url':
+                if ( !preg_match('/^(?:http|https):\/\/[\w,.:;&=+*%$#!?@()~\'\/-]+$/', $str )) {
+                    $this->setErrorMessage( $name, 'invalid', $val );
+                }
+                break;
+            case 'phone':
+                $number = preg_replace('/[^\d]+/', '', $str );
+                if (!('0' === $number{0} && (10 == strlen($number) || 11 == strlen($number)) ) )  {
+                    $this->setErrorMessage( $name, 'invalid', $val );
+                }
+                break;
+        }
+    }
+    
+    /*
+     * エラーメッセージをセットする
+    */
+    private function setErrorMessage( $name, $type, $validationElement = null ) {
+        if (!isset($this->errorMessages[$name])) {
+            $this->errorMessages[$name] = array();
+        }
+        if( isset($validationElement[$type]['err_msg']) ) {
+            $this->errorMessages[$name][$type] = $validationElement[$type]['err_msg'];
+        } else {
+            $this->errorMessages[$name][$type] = $this->defaultErrorMsg[$type];
+        }
+    }
+    
+    public function getErrorMessages() {
+        return $this->errorMessages;
+    }
+}
\ No newline at end of file
diff --git payment/pay/common/class/application/mediaplan/model/dao/MediaPlanApplicationDao.php payment/pay/common/class/application/mediaplan/model/dao/MediaPlanApplicationDao.php
new file mode 100755
index 0000000..a57a823
--- /dev/null
+++ payment/pay/common/class/application/mediaplan/model/dao/MediaPlanApplicationDao.php
@@ -0,0 +1,75 @@
+<?php
+require_once CLASS_DIR.'/PremiumDbAccess.php';
+
+class MediaPlanApplicationDao extends PremiumDbAccess {
+    public function __construct() {
+        // DB接続周りだけを借りるので、親クラスの__constract()の初期化は用いない
+    }
+
+    /**
+     * チャンネル申請の申し込みの状況（ステップ状態）を得る
+     * @return string 対応するownerIdのapply_divの値。存在しなければnull
+     */
+    public function findByOwnerId($ownerId) {
+        $dbh = $this->getDBH( self::DB_CHANNEL_SLAVE );
+        return $this->findByOwnerIdImpl($dbh, $ownerId);
+    }
+    
+    private function findByOwnerIdImpl($dbh, $ownerId) {
+        $applicateTimeout = CH_APPLICATE_TIMEOUT;
+        $sql = "
+        SELECT * FROM channel_mediaplan_application
+        WHERE
+            owner_id = :owner_id
+        AND register_time > DATE_SUB(NOW(), INTERVAL $applicateTimeout HOUR)";
+        $sth = $this->prepare( $dbh, $sql );
+        $sth->execute( array( ':owner_id' => $ownerId ) );
+        return $sth->fetch( PDO::FETCH_ASSOC );
+    }
+
+    /**
+     * channel_mediaplan_application にデータを挿入する
+    */
+    public function insert(array $params) {
+        $dbh = $this->getDBH(self::DB_CHANNEL_MASTER);
+        $this->setCurrentDateTime();
+        
+        // 検索して apply_divがAPPLYING だったら実行しない
+        $status = $this->findByOwnerIdImpl($dbh, $params[':owner_id']);
+        if( $status !== null && $status['apply_div'] == 'APPLYING' ) {
+            return false;
+        }
+        try {
+            $sql = "
+            INSERT INTO channel_mediaplan_application (
+                owner_id,
+                owner_mail,
+                old_blog_name,
+                old_blog_url,
+                old_blog_daily_pv,
+                new_blog_name,
+                note,
+                ip_address,
+                apply_div,
+                update_time
+            ) VALUES (
+                :owner_id,
+                :owner_mail,
+                :old_blog_name,
+                :old_blog_url,
+                :old_blog_daily_pv,
+                :new_blog_name,
+                :note,
+                :ip_address,
+                'APPLYING',
+                NOW()
+            )";
+            $sth = $this->prepare( $dbh, $sql );
+            $sth->execute($params);
+            return true;
+        } catch (Exception $ex) {
+            $this->makeErrorLog($ex, 'チャンネル開設申請　登録');
+            throw $ex;
+        }
+    }
+}
