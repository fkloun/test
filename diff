+<?php
+require_once dirname(__FILE__) . '/MediaPlanControllerBase.php';
+require_once dirname(__FILE__).'/../model/MediaPlanApplicationRepository.php';
+require_once dirname(__FILE__).'/../../dao/CommunityDao.php';
+
+class MediaPlanGateController extends MediaPlanControllerBase {
+    public static function factory() {
+        return new self();
+    }
+    public function executeImpl() {
+        // 本家認証の戻りURLが指定あった場合、そこにジャンプさせる
+        if ( !empty($this->get['next_url']) ) {
+            header('Location: /' . $this->get['next_url']);
+            return;
+        }
+        if ( isset( $this->post['mode'] ) && $this->post['mode'] === 'logout' ) {
+            util::jumpNicoAuth();
+            return;
+        }
+        
+        $this->clearAllCookies();
+        $entity = MediaPlanApplicationRepository::factory()->getByOwnerId($this->userInfo['id']);
+        if ( $entity->isApplying() ) {
+            // APPLYING状態だと待ち表示
+            $this->view->assign('is_inspection', true);
+            $this->view->assign('is_entry_inspection', true);
+        }
+        
+        $ownChannelNames = CommunityDao::factory()->findChannelNamesByOwnerId($this->userInfo['id']);
+        $this->view->assign('ownChannelNames', $ownChannelNames);
+        
+        $this->bodyTpl = 'application/mediaplan/gate.tpl';
+    }
+}
