diff --git payment/pay/common/class/application/dao/CommunityDao.php payment/pay/common/class/application/dao/CommunityDao.php
new file mode 100755
index 0000000..d7d7937
--- /dev/null
+++ payment/pay/common/class/application/dao/CommunityDao.php
@@ -0,0 +1,58 @@
+<?php
+require_once CLASS_DIR.'/PremiumDbAccess.php';
+/**
+ * communityテーブルのデータアクセスオブジェクト
+ * @author manato_kato
+ */
+class CommunityDao extends PremiumDbAccess {
+    public function __construct() {}
+    
+    public static function factory() {
+        return new self();
+    }
+    
+    /**
+     * @param int $ownerId チャンネルのオーナーID
+     * @return array:string オーナーIDに紐づくチャンネルの名前
+     */
+    public function findChannelNamesByOwnerId($ownerId) {
+        $dbh = $this->getDBH( self::DB_COMMUNITY_SLAVE );
+        $sql = <<<SQL
+            SELECT name
+            FROM community
+            WHERE
+                user_id=:user_id
+                AND channel_id IS NOT NULL
+SQL;
+        $sth = $this->prepare( $dbh, $sql );
+        $sth->execute( array( ':user_id' => $ownerId ));
+        $fetchResult = $sth->fetchAll();
+        
+        // 名前だけ欲しいので、配列に詰め直す
+        $names = array();
+        foreach($fetchResult as $r) {
+            $names[] = $r['name'];
+        }
+        return $names;
+    }
+    
+    /**
+     * 引数のオーナーIDのユーザーが既にチャンネルを持っているか調べる
+     * @param int $ownerId チャンネルのオーナーID
+     * @return 既にチャンネルを持っていればtrue
+     */
+    public function findOwnChannel($ownerId) {
+        $dbh = $this->getDBH( self::DB_COMMUNITY_SLAVE );
+        $sql = <<<SQL
+            SELECT count(id)
+            FROM community
+            WHERE
+                user_id=:user_id
+                AND channel_id IS NOT NULL
+SQL;
+        $sth = $this->prepare( $dbh, $sql );
+        $sth->execute( array( ':user_id' => $ownerId ));
+        $fetchResult = $sth->fetch(PDO::FETCH_ASSOC);
+        return $fetchResult['count(id)'] !== '0';
+    }
+}
\ No newline at end of file
