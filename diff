diff --git payment/pay/htdocs/js/application/mediaplan.js payment/pay/htdocs/js/application/mediaplan.js
new file mode 100755
index 0000000..6cefcf4
--- /dev/null
+++ payment/pay/htdocs/js/application/mediaplan.js
@@ -0,0 +1,155 @@
+jQuery(function($) {
+    /**
+     * 送信前にチェックする値について取得
+     * @return Object フォームのname->値な連想配列
+     */
+    var getFormValues = function() {
+        var result = {};
+        var addFormValue = function(name) {
+            var $dom = $("#"+name);
+            if ($dom.val() !== undefined) {
+                result[name] = $dom.val();
+            }
+        };
+        addFormValue('old_blog_name');
+        addFormValue('old_blog_url');
+        addFormValue('old_blog_daily_pv');
+        addFormValue('new_blog_name');
+        addFormValue('note');
+        return result;
+    };
+    
+    /**
+     * フォームデータの内容を送信する
+     * @param string フォームオブジェクト
+     */
+    var sendFormValues = function(form, formValues){
+        $.each(formValues, function(name, value) {
+            form.appendChild(createInputData(name, value));
+        });
+        form.submit();
+    };
+    
+    /**
+     * フォームデータの内容をチェックする
+     * @param form フォームオブジェクト
+     */
+    var hasNoError = function(formValues){
+        return checkEmpty(formValues)
+            && checkNumericAtom(formValues)
+            && checkLengthArguments(formValues)
+            && checkIPAddress(formValues)
+            && checkUrl(formValues)
+            && checkPhoneNumber(formValues)
+            && checkChannelName()
+            && checkKanaArguments(formValues)
+            && checkHuriKana();
+    };
+    $('#confirm_button').click(function() {
+        $('.errormsg').hide();
+        var formValues = getFormValues();
+        if (hasNoError(formValues)) {
+            sendFormValues(this.form, formValues);
+        } else {
+            warningScroll();
+        }
+    });
+
+    // 確認用
+    $('#submit_button').click(function() {
+        if( !$('#agreement_check').is(':checked') ) {
+            $('#error_need_agreement').show();
+            return;
+        }
+        sendFormValues(this.form, getFormValues());
+    });
+    
+    // もどるボタン用
+    $('#back_button').click(function() {
+        $(this.form).attr('action', $(this).attr("formaction"));
+        sendFormValues(this.form, getFormValues());
+    });
+
+    $('#agreement_check').click(function() {
+        $('#error_need_agreement').hide();
+    });
+});
+
+
+//-------- 以下は app_common.js で利用される --------
+var OBJ = {};
+/*
+ * 各入力受付データについて
+ * 受け付ける長さ
+ */
+OBJ.LENGTH_DATA = {
+    'old_blog_name'           : 255,
+    'old_blog_url'            : 255,
+    'old_blog_daily_pv'       : 64,
+    'new_blog_name'           : 248,
+    'note'                    : 2048
+};
+
+/**
+ * 長さチェックをスルーするフィールドを定義
+ *
+ * @return Object オブジェクト
+ */
+function getThruLengthKeys() {
+    return {};
+}
+
+/**
+ * 空欄チェックを行うフィールドを定義
+ *
+ * @return Object オブジェクト
+ */
+function getEmptyKeys() {
+    return [
+        'old_blog_name',
+        'old_blog_url',
+        'old_blog_daily_pv',
+        'new_blog_name'
+    ];
+}
+
+/**
+ * 数値チェックを行うフィールドを定義
+ *
+ * @return Object オブジェクト
+ */
+function getCheckNumericKeys() {
+    return [];
+}
+
+/**
+ * フリカナチェックを行うフィールドを定義
+ * @return Object オブジェクト
+ */
+function getCheckKanaKeys() {
+    return [];
+}
+
+/**
+ * 口座チェックを行うフィールドを定義
+ * @return Object オブジェクト
+ */
+function getCheckAccountFields() {
+    return {};
+}
+
+function getIpAddrKeys() {
+    return [];
+}
+
+function getUrlKeys() {
+    return ['old_blog_url'];
+}
+
+function getPhoneNumberKeys() {
+    return [];
+}
+
+function getChannelName() {
+    return 'new_blog_name';
+}
