+<?php
+require_once dirname(__FILE__).'/MediaPlanControllerBase.php';
+
+/**
+ * メディアプラン静的ページ用のコントローラ
+ * @author manato_kato
+ */
+class MediaPlanStaticPageController extends MediaPlanControllerBase {
+    public static function factory() {
+        return new self();
+    }
+    
+    public function executeImpl() {
+        // request_uriを用いてファイル名を取得し、同名のtplをbodyTplに設定する
+        if (!isset($this->server['REQUEST_URI'])) {
+            return;
+        }
+        $fileName = preg_replace('#/?[^/]+/#', '', $this->server['REQUEST_URI']);
+        if ($fileName === '') {
+            $fileName = 'index';
+        }
+        $this->bodyTpl = 'application/mediaplan/'.$fileName.'.tpl';
+    }
+}
