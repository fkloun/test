+                    //            'invalid'   => array( 'value' => true, 'err_msg' => '書式に誤りがあります。' ),
+                    //        ),
+
+    );
+
+    /*
+     * err_msg を指定しなかった場合のデフォルトのエラーメッセージ
+    */
+    protected $defaultErrorMsg = array(
+            'isRequire' => '入力してください。',
+    );
+
+
+    /*
+     * コンストラクタ・ファクトリ・getter/setter等
+    */
+    public function __construct( $params, $userInfo ) {
+        $this->setParams( $params );
+        $this->setUserInfo( $userInfo );
+    }
+
+    public function factory( $params, $userInfo ) {
+        return new self( $params, $userInfo );
+    }
+
+    public function setParams( $params ) {
+        $this->params = $params;
+        return $this;
+    }
+
+    public function getParams() {
+        return $this->filterByElements( $this->params );
+    }
+
+    public function getUserInfo() {
+        return $this->userInfo;
+    }
+
+    public function setUserInfo( $userInfo ) {
+        $this->userInfo = $userInfo;
+        return $this;
+    }
+
+    public function getElements() {
+        return $this->elements;
+    }
+
+    public function getElement( $name ) {
+
+        if( isset( $this->elements[$name] ) ) {
+            return $this->elements[$name];
+        } else {
+            return null;
+        }
+    }
+
+    public function getElementNames() {
+        return array_keys($this->elements);
+    }
+
+    public function filterByElements( $params ) {
+        $paramsTemp = array();
+        foreach( $params as $key => $val ) {
+            //            $element = $this->getElement( $key );
+            if( isset( $this->elements[$key] ) ) {
+                $paramsTemp[$key] = $val;
+            }
+        }
+        return $paramsTemp;
+    }
+
+    public function getExternalLinkFields() {
+
+        return array (
+                '', // dummy
+                'external_link1',
+                'external_link2',
+                'external_link3',
+        );
+    }
+
+    /*
+     * 希望開設日のリストを返す
+    * 14～90日内の月曜日を返す
+    */
+    public function getPublishDates() {
+
+        $dateList[0] = array('id' => 0, 'name' => '選択してください');
+
+        date_default_timezone_set('Asia/Tokyo');    // タイムゾーンを指定しないと、サマータイムで１時間ズレるとかで無駄に悩む
+
+        $ymd = date( "Ymd" );
+        $timestamp = strtotime( $ymd );
+        $res = date( "w", $timestamp );
+
+        // 今日から起算して何日足すと次の月曜日になるかという値
+        if ($res === '0') {
+            $offset = 1;
+        } elseif ( $res === '1') { // 14日目の月曜日は通す
+            $offset = 0;
+        } else {
+            $offset = 8 - $res;
+        }
+        $timestamp += ((14 + $offset) * 86400); // 次の月曜日のタイムスタンプ
+        $rest = 90 - 14 - $offset; // 2週間以降最初の月曜日から、90日目までの日数
+
+        $count = 1;
+        while ( $rest >= 0) {
+            $dateList[$count] = array('id' => $count, 'name' => date('Y/m/d', $timestamp) );
+            $count ++;
+            $timestamp += (7 * 86400);
+            $rest -= 7;
+        }
+
+        return $dateList;
+    }
+
+    /*
+     * バリデーション
+    */
+    public function validate() {
+
+        $paramsTemp = $this->params;
+        foreach( $this->elements as $key => $val ) {
+
+            // 入力必須のチェック
+            if ( isset($val['isRequire']['value']) && $val['isRequire']['value'] ) {
+                //echo("require");
+                if ( empty($paramsTemp[$key]) && $paramsTemp[$key] !== '0' && $paramsTemp[$key] !== '' ) {
+                    // セットされていない
+                    //echo("no set !!!!!");
+                    $name = $key;
+                    $type = 'isRequire';
+                    $msg = $val[$type]['err_msg'];
+                    $this->setErrorMsg( $name, $type, $msg );
+                    $this->isError = true;
+                }
+
+            } else {
+                //echo("no require");
+            }
+
+
+            $str = $paramsTemp[$key];
+
+            // 入力必須でない項目が空なら、バリデーションの対象としない
+            if( ( empty( $val['isRequire']['value'] ) || !$val['isRequire']['value'] ) && (empty($str) || trim($str) === '' ) ) {
+                continue;
+            }
+
+            switch ( $val['type']) {
+                case 'input':
+                    // チャンネル名だったら
+                    if( isset($val['isChannelName']['value']) && $val['isChannelName']['value'] ) {
+
+                        if( !util::checkChannelName( $str ) ) {
+                            $name = $key;
+                            $type = 'invalid';
+                            $msg = $val['isChannelName']['err_msg'];
+                            $this->setErrorMsg( $name, $type, $msg );
+                            $this->isError = true;
+                        }
+                    }
+
+                    break;
+
+                case 'text':
+                    break;
+
+                case 'url':
+                    $pattern = '/^(?:http|https):\/\/[\w,.:;&=+*%$#!?@()~\'\/-]+$/';
+                    if ( !preg_match($pattern, $str )) {
+                        $name = $key;
+                        $type = 'invalid';
+                        $msg = $val[$type]['err_msg'];
+                        $this->setErrorMsg( $name, $type, $msg );
+                        $this->isError = true;
+                    }
+                    break;
+
+                case 'phone':
+                    $number = preg_replace('/[^\d]+/', '', $str );
+                    if (! ('0' === $number{0} && (10 == strlen($number) || 11 == strlen($number)) ) )  {
+                        $name = $key;
+                        $type = 'invalid';
+                        $msg = $val[$type]['err_msg'];
+                        $this->setErrorMsg( $name, $type, $msg );
+                        $this->isError = true;
+                    }
+                    break;
+
+                case 'radio':
+                    break;
+
+                default:
+                    $this->isError = true;
+            }
+        }
+
+        if ( $this->isError ) {
+            throw new MyException( MyException::ERROR_PARAM );
+        }
+
+        return $this;
+    }
+
+    /*
+     * 内部用 エラーメッセージをセットする
+    */
+    private function setErrorMsg( $name, $type, $msg = null ) {
+
+        if( isset( $msg ) ) {
+            $this->errorMsgs[$name][$type] = $msg;
+        } else {
+            $this->errorMsgs[$name][$type] = $this->defaultErrorMsg[$type];
+        }
+        return $this;
+    }
+
+    public function getErrorMsgs() {
+        return $this->errorMsgs;
+    }
+}
+
