+<?php
+// ↓はないと呼ばれてる関連クラスでコケるため、しかたなく呼んでいるが、可能なら消したい
+chdir('../../');
+require_once dirname(__FILE__).'/../../../conf/main_conf.php';
+require_once CLASS_DIR.'/payment_util.php';
+require_once CLASS_DIR.'/db_access.php';
+require_once CLASS_DIR.'/error_class.php';
+require_once CLASS_DIR.'/util.php';
+require_once COMMON_DIR.'/lib/niwavide.status.php';
+
+if (false === defined('CHT_DATABASE_USERNAME')) {
+    define('CHT_DATABASE_USERNAME', FRONTPAYUSER);
+    define('CHT_DATABASE_PASSWORD', FRONTPAYPW);
+}
+require_once COMMON_DIR.'/lib/niwavide_cht.util.php';
+
+/**
+ * メディアプランデジタル開設共通ディスパッチャ
+ * @author manato_kato
+ */
+class MediaPlanDispatcher {
+    private $controllerName;
+    private $userInfo = null;
+    public function __construct($controllerName) {
+        $this->controllerName = $controllerName;
+    }
+
+    /**
+     * メンテ処理・ログイン確認等
+     */
+    public function checkAuth($nextUrl=null) {
+        global $urgent_mode, $payment_urgent_mode;
+        $params = compact( $urgent_mode, $payment_urgent_mode );
+        if ($nextUrl !== null) {
+            util::userAuth($params, true, true, $nextUrl);
+        } else  {
+            util::userAuth($params);
+        }
+        $this->userInfo = $this->getUserInfo();
+    }
+
+    /**
+     * ユーザ情報を得る
+     */
+    private function getUserInfo() {
+        $userInfo = paymentUtil::getUserInfomationCookie();
+        if( $userInfo === false ) {
+            throw new MyException( MyException::ERROR_COOKIE );
+        }
+        return $userInfo;
+    }
+
+    public function run() {
+        require_once(dirname(__FILE__) . "/controller/{$this->controllerName}.php");
+        $controller = call_user_func(array($this->controllerName, 'factory'));
+        $controller->setParams(array(
+            'get'    => $_GET,
+            'post'   => $_POST,
+            'cookie' => $_COOKIE,
+            'server' => $_SERVER
+        ));
+        $controller->setUserInfo($this->userInfo);
+        $controller->execute();
+    }
+}
