+<?php
+require_once dirname(__FILE__) . '/MediaPlanApplicationController.php';
+
+/**
+ * メディアプラン開設申請(Application)詳細
+ * @author manato_kato
+ */
+class MediaPlanApplicationDetail extends MediaPlanApplicationController {
+    protected function executeImpl($smarty) {
+        $this->setPageTitle('メディアプラン開設申請');
+        $this->setSubTitle('詳細');
+
+        $app = $this->getApplicationEntity();
+        if ($app === null) {
+            header("Location: /application/mediaplan/view.php");
+            return;
+        }
+        $smarty->assign('app', $app);
+     }
