diff --git chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailComplete.php chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailComplete.php
new file mode 100755
index 0000000..b574e2b
--- /dev/null
+++ chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailComplete.php
@@ -0,0 +1,83 @@
+<?php
+require_once dirname(__FILE__) . '/MediaPlanApplicationController.php';
+require_once dirname(__FILE__) . '/dao/MediaPlanOpenManager.php';
+require_once dirname(__FILE__) . '/dao/MediaPlanApplicationDao.php';
+
+
+/**
+ * 大手メディアプラン開設申請(アプリケーション)のメール送信完了時アクション
+ * @author manato_kato
+ */
+class MediaPlanApplicationMailComplete extends MediaPlanApplicationController {
+    // レプリケーション遅延でログインコードが取得できなかった場合に、リトライする回数
+    const REPLICATION_RETRY_COOUNT = 5;
+    
+    public function executeImpl($smarty) {
+        $app = $this->getApplicationEntity();
+        if ($app === null) {
+            return;
+        }
+
+        $investigation = $app['apply_div'];
+        // 申請中の場合、ステータスの更新と、必要ならチャンネルの作成をする。
+        if ($app['apply_div'] === 'APPLYING') {
+            $investigation = $this->getInvestigation();
+            $id = intval($this->getParam('id'));
+            // ツール発行申請許可の場合チャンネルを作成する
+            if ($investigation === 'OK') {
+                MediaPlanOpenManager::register($app, $this->session);
+            }
+            $this->updateOpenApplyDiv($id, $investigation);
+        }
+        
+        $this->sendMail(
+            $app['owner_mail'],
+            htmlspecialchars($this->getParam('mail_body')),
+            $investigation === 'OK' ? $this->getLoginCode($app['owner_id']) : ''
+        );
+        header("Location: /application/mediaplan/view.php");
+    }
+    
+    private function updateOpenApplyDiv($id, $investigation) {
+        $dao = new MediaPlanApplicationDao();
+        $dao->updateApplyDivStatus($id, $investigation, date('Y-m-d H:i:s'));
+    }
+    
+    private function getLoginCode($ownerId) {
+        $toolUserDao = new ChannelToolUserDao();
+        for ($i = 0; $i < self::REPLICATION_RETRY_COOUNT; $i++) {
+            $loginCode = $toolUserDao->getLoginCode($ownerId);
+            if ($loginCode !== null) {
+                return $loginCode;
+            }
+            sleep(1);
+        }
+        throw new Exception("チャンネルツール・ログインコードを取得出来ませんでした。");
+    }
+
+    /**
+     * メール送信処理
+     */
+    private function sendMail($ownerMail, $mailBody, $loginCode) {
+        switch (php_uname("n")) {
+            // 本番環境
+            case "ch-manage":
+                $toolUrl = 'https://chtool.nicovideo.jp/login/' . $loginCode;
+                $title = '【ニコニコチャンネル】審査結果';
+                $infoTo = 'p-nicoch-blog-establish-ext@dwango.co.jp';
+                break;
+            // dev・ローカル環境など、本番以外
+            default:
+                $toolUrl = 'https://chtool.dev.nicovideo.jp/login/' . $loginCode;
+                $title = 'テスト【ニコニコチャンネル】審査結果';
+                $infoTo = 'manato_kato@dwango.co.jp';
+                break;
+        }
+        $mailBody = str_replace('#管理ツールURL#', $toolUrl, $mailBody);
+        // CPさんへ
+        ChannelUtils::sendMail('no-reply@dwango.co.jp', $ownerMail, $title, $mailBody);
+        // 運営MLへ
+        ChannelUtils::sendMail('no-reply@dwango.co.jp', $infoTo, $title, $mailBody);
+    }
+
+}
diff --git chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailEdit.php chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailEdit.php
new file mode 100755
index 0000000..5fee8e1
--- /dev/null
+++ chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationMailEdit.php
@@ -0,0 +1,44 @@
+<?php
+require_once dirname(__FILE__) . '/MediaPlanApplicationController.php';
+
+/**
+ * メディアプラン開設申請(Application)返信用メール編集画面
+ * @author manato_kato
+ */
+class MediaPlanApplicationMailEdit extends MediaPlanApplicationController {
+    const PAGE_EDIT = 'edit';
+    const PAGE_CONFIRM = 'confirm';
+    const PAGE_SEND_MAIL = 'send_mail';
+
+    protected function executeImpl($smarty) {
+        $app = $this->getApplicationEntity();
+        if ($app === null) {
+            header("Location: /application/mediaplan/view.php");
+            return;
+        }
+        $this->setPageTitle('メディアプラン開設申請');
+        $smarty->assign('app', $app);
+
+        // DBに入っている状態が審査中なら審査中と表示し、そうでなければ再送モード
+        if ($app['apply_div'] === 'APPLYING') {
+            $this->setSubTitle('審査');
+            $smarty->assign('investigation',  $this->getInvestigation());
+        } else {
+            $this->setSubTitle('メール再送');
+            $smarty->assign('investigation',  $app['apply_div']);
+        }
+        
+        if (isset($this->params['mail_body'])) {
+            $smarty->assign('mail_body', htmlspecialchars($this->getParam('mail_body')));
+        }
+        
+        switch ($this->getParam('nextPage')) {
+            case self::PAGE_CONFIRM:
+                $smarty->assign('currentPage', self::PAGE_CONFIRM);
+                break;
+            default:
+                $smarty->assign('currentPage', self::PAGE_EDIT);
+                break;
+        }
+    }
+}
diff --git chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationView.php chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationView.php
new file mode 100755
index 0000000..dd1df79
--- /dev/null
+++ chmanage/pay/common/class/application/mediaplan/MediaPlanApplicationView.php
@@ -0,0 +1,55 @@
+<?php
+require_once dirname(__FILE__) . '/MediaPlanApplicationController.php';
+
+/**
+ * メディアプラン開設申請(Application)一覧
+ * @author manato_kato
+ */
+class MediaPlanApplicationView extends MediaPlanApplicationController {
+    const ENTRY_PER_PAGE = 50;
+    
+    const APPLY_DIV_ALL = '';
+    const APPLY_DIV_APPLYING = 'APPLYING';
+    const APPLY_DIV_OK = 'OK';
+    const APPLY_DIV_NG = 'NG';
+    
+    protected function executeImpl($smarty) {
+        $this->setPageTitle('大手メディアプラン開設申請');
+
+        $searchArray = array();
+        if (isset($this->params['search'])) {
+            $searchArray = preg_split('/ |　/', htmlspecialchars($this->params['search']));
+        }
+
+        // page番号取得
+        $page = isset($this->params['page']) ? intval($this->params['page']) : 1;
+        $page = $page <= 0 ? 1 : $page;
+        $smarty->assign('page', $page);
+
+        $dao = new MediaPlanApplicationDao();
+        $applications = $dao->findApplications($searchArray, $this->getApplyDiv($this->params), self::ENTRY_PER_PAGE, self::ENTRY_PER_PAGE * ($page - 1));
+
+        $smarty->assign('applications', $applications);
+        $smarty->assign('applying_count', $dao->findCountByApplyDiv('APPLYING'));
+
+        // Pager作成
+        $pagar = $this->createPager($page, self::ENTRY_PER_PAGE, $dao->findCountByApplyDiv());
+        $smarty->assign('navigation', $pagar);
+        
+        $this->assignParams($smarty);
+    }
+    
+    private function getApplyDiv($params) {
+        $applyDivsExpected = array(self::APPLY_DIV_APPLYING, self::APPLY_DIV_NG, self::APPLY_DIV_OK);
+        if (isset($params['apply_div']) && in_array($params['apply_div'], $applyDivsExpected)) {
+            return $params['apply_div'];
+        }
+        return self::APPLY_DIV_ALL;
+    }
+    
+    private function assignParams($smarty) {
+        $smarty->assign('search', isset($this->params['search']) ? $this->params['search'] : '');
+        $smarty->assign('applyDiv', $this->getApplyDiv($this->params));
+    }
+
+}
diff --git chmanage/pay/common/class/application/mediaplan/dao/MediaPlanApplicationDao.php chmanage/pay/common/class/application/mediaplan/dao/MediaPlanApplicationDao.php
new file mode 100755
index 0000000..ecd2a16
--- /dev/null
+++ chmanage/pay/common/class/application/mediaplan/dao/MediaPlanApplicationDao.php
@@ -0,0 +1,128 @@
+<?php
+require_once CLASS_DIR . '/db/PaymentSlaveDatabase.class.php';
+require_once dirname(__FILE__) . '/../../db/PaymentMasterDB.php';
+
+
+/**
+ * メディアプランの申請情報(Application)のDAO
+ * @author manato_kato
+ */
+class MediaPlanApplicationDao extends PaymentSlaveDatabase {
+    /**
+     * メディアプラン申請情報のapplyDivとapprovalTimeを更新する
+     * @param int $id
+     * @param string $applyDiv
+     * @param string $approvalTime
+     * @throws DataAccessException
+     */
+    public function updateApplyDivStatus($id, $applyDiv, $approvalTime=null) {
+        $approvalTimeCondition = $approvalTime !== null ? "approval_time = '{$approvalTime}'," : '';
+        $sql = <<<QUERY
+            UPDATE
+                channel_mediaplan_application
+            SET
+                apply_div = :apply_div,
+                {$approvalTimeCondition}
+                update_time = NOW()
+            WHERE
+                id = :id
+QUERY;
+        $db = PaymentMasterDB::getInstance();
+        $sth = $db->prepare($sql);
+        $sth->bindParam(':apply_div', $applyDiv, PDO::PARAM_STR);
+        $sth->bindParam(':id',        $id,       PDO::PARAM_INT);
+        if ($db->execute($sth) === false) {
+            throw new DataAccessException('ステータス更新に失敗しました');
+        }
+    }
+    
+    /**
+     * Applyingの状態によってエントリの数をカウントする
+     * @param string $applying
+     * @return int
+     */
+    public function findCountByApplyDiv($applyDiv = '') {
+        $whereClause = '';
+        $params = array();
+        $applyDivCondition = $this->createApplyDivCondition($applyDiv, $params);
+        $sql = <<<QUERY
+            SELECT count(*)
+            FROM channel_mediaplan_application
+            WHERE {$applyDivCondition}
+QUERY;
+        $sth = $this->prepare($sql);
+        $sth->execute($params);
+        $result = $sth->fetch();
+        return (int)$result['count(*)'];
+    }
+    
+    /**
+     * idにより対応する申請情報を取得。
+     * @param int $id
+     */
+    public function findById($id) {
+        $sql =
+<<<QUERY
+            SELECT *
+            FROM channel_mediaplan_application
+            WHERE id = :id
+QUERY;
+        $sth = $this->prepare($sql);
+        $sth->execute(array( ':id' => $id ));
+        return $sth->fetch();
+    }
+    
+    /**
+     * 指定した条件で申請情報の配列を得る。
+     * @param array $searchWords 検索条件の単語のリスト
+     * @param string $applyDiv OK|NG 空文字で条件なし
+     * @param int $limit  引いてくる上限個数
+     * @param int $offset SQLのLIMIT句のOFFSET
+     */
+    public function findApplications(array $searchWords = array(), $applyDiv = '', $limit = null, $offset = null) {
+        $params = array();
+        $searchCondition = $this->createSearchCondition($searchWords, $params);
+        $applyDivCondition = $this->createApplyDivCondition($applyDiv, $params);
+        $limitClause = ($limit !== null && $offset !== null) ? " LIMIT {$limit} OFFSET {$offset}" : "";
+
+        $sql =
+<<<QUERY
+            SELECT *
+            FROM channel_mediaplan_application
+            WHERE {$searchCondition} AND {$applyDivCondition}
+            ORDER BY
+                update_time DESC
+            {$limitClause}
+QUERY;
+        $sth = $this->prepare($sql);
+        $sth->execute($params);
+        return $sth->fetchAll();
+    }
+
+    private function createApplyDivCondition($applyDiv, array &$params) {
+        if ($applyDiv !== '') {
+            $params[':apply_div'] = $applyDiv;
+            return "apply_div=:apply_div";
+        }
+        return '1';
+    }
+
+    /**
+     * 検索する文字列のリストからSQLの検索条件を作り、更に引数の$paramsにパラメータを追加する。
+     * @param array $searchWords
+     * @param array $params
+     * @return string
+     */
+    private function createSearchCondition(array $searchWords, array &$params) {
+        $searchCondition = array();
+        foreach ($searchWords as $key => $value) {
+            $paramName = ':search'.$key;
+            $searchCondition []= "(owner_mail LIKE $paramName OR old_blog_name LIKE $paramName OR new_blog_name LIKE $paramName OR old_blog_url LIKE $paramName)";
+            $params[$paramName] = '%'.$value.'%';
+        }
+        if (count($searchCondition) === 0) {
+            return '1';
+        }
+        return implode(' AND ', $searchCondition);
+    }
+}
diff --git chmanage/pay/common/class/application/mediaplan/dao/MediaPlanOpenManager.php chmanage/pay/common/class/application/mediaplan/dao/MediaPlanOpenManager.php
