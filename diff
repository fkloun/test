+<?php
+/**
+ * メディアプラン申請データのエンティティ。
+ * @author manato_kato
+ */
+class MediaPlanApplicationEntity {
+    private $params;
+    public function __construct(array $params = null) {
+        $this->params = $params;
+    }
+    
+    public function getParams() {
+        return $this->params;
+    }
+    
+    public function isApplying() {
+        return $this->params !== null && $this->params['apply_div'] === 'APPLYING';
