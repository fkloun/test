+    public function modeExecute( $mode ) {
+        switch ( $mode ) {
+            // 編集モード
+            case EDIT_MODE:
+                break;
+
+            // 新規追加モード
+            case CREATE_MODE:
+                break;
+            // 削除モード
+            case DELETE_MODE:
+                break;
+
+            // 最終確認モード
+            case CHECK_MODE:
+                break;
+
+            // 編集追加時のモード (writedata)
+            case EDIT_NOW:
+                // バリデーション
+                try {
+                    $this->model->validate();
+
