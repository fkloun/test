+<?php
+require_once(HOME_DIR . '/pay/common/class/chstatus_util.php');
+require_once(HOME_DIR . '/pay/common/class/companyinfo/CompanyInfo.php');
+require_once(HOME_DIR . '/pay/common/class/dao/CommunityDao.php');
+require_once(HOME_DIR . '/pay/common/class/dao/ChannelToolUserDao.php');
+require_once(HOME_DIR . '/pay/common/conf/channel_info_conf.php');
+require_once(HOME_DIR . '/pay/common/class/manage_support_account/SupportAccountCall.class.php');
+require_once(HOME_DIR . '/pay/common/class/error_class.php');
+
+require_once Path::getModelDir() . '/ChannelScreenNameCheckModel.php';
+
+/**
+ * メディアプラン新規開設処理担当さん
+ * @author manato_kato
+ */
+class MediaPlanOpenManager {
+    public static function register(array $app, $session = null) {
+        $masterDB = MasterDB::getInstance();
+        $paymentMasterDB = PaymentMasterDB::getInstance();
+
+        $masterDB->beginTransaction();
+        $paymentMasterDB->beginTransaction();
+        try {
+            self::registerOwnerInfo($app);
+            self::registerChannelInfo($app, $session);
+            $masterDB->commit();
+            $paymentMasterDB->commit();
+        } catch (Exception $e) {
+            $masterDB->rollBack();
+            $paymentMasterDB->rollBack();
+            throw $e;
+        }
+    }
+
+    private static function registerOwnerInfo(array $app) {
+        $toolUserDao = new ChannelToolUserDao();
+        $existsOwner = $toolUserDao->exists(intval($app['owner_id']));
+        /**
+         * EDIT_NEWは新規追加モードで、オーナーアカウントではないアカウントによる登録、
+         * EDIT_NOWは編集モードで、オーナーアカウントが新しくチャンネルを作る場合
+         * らしい・・・
+         */ 
+        $mode = $existsOwner ? EDIT_NOW : EDIT_NEW;
+        
+        $ownerData = array(
+            "mode"              => $mode,
+            "company_name"      => $app['old_blog_name'].'(ブロマガ大手メディアサイトプラン)',
+            "company_nickname"  => $app['old_blog_name'],
+            "owner_id"          => $app['owner_id'],
+            "zip_code"          => '000-0000',
+            "address1"          => '東京都',
+            "address2"          => '江東区',
+            "division_name"     => '',
+            "person"            => '',
+            "phone_no"          => '000-0000-0000',
+            "fax_no"            => '',
+            "email"             => $app['owner_mail'],
+            "fb_account_code"   => '',
+            "bank_id"           => '',
+            "bank_kana"         => '',
+            "bank_name"         => '',
+            "branch_code"       => '',
+            "branch_kana"       => '',
+            "branch_name"       => '',
+            "account_type"      => '',
+            "account_no"        => '',
+            "account_name"      => '',
+            "account_kana"      => '',
+            "support_email"     => $app['owner_mail'],
+            "recital"           => $app['note'],
+            "rcp_company_id"    => null,
+            "contract_status"   => 0,
+            "description"       => '',
+            "login_enable_addr" => ''
+        );
+        // channelestablish, channeltooluserにINSERT
+        $companyInfoObj = new companyInfo();
+        $companyInfoObj->writingChannelData($ownerData, 'admin');
+    }
+
+    private static function registerChannelInfo(array $app, $session = null) {
+        $comDao = new CommunityDao(ComSlaveDB::getInstance());
+        $channelId = $comDao->getEmptyChannelId();
+
+        $channelData = array(
+            'account_id'              => $session['user_id'],
+            "channel_id"              => $channelId,
+            "name"                    => $app['old_blog_name'],
+            "description"             => '',
+            "user_id"                 => $app['owner_id'],
+            "type"                    => 'normal',
+            "price_credit"            => 0,
+            "price_web"               => 0,
+            "price_mobile"            => 0,
+            "distribution"            => 0,
+            "distribution_ppv"        => 0,
+            "option_flag"             => '00001',
+            "hidden"                  => 1,
+            "official"                => 1,
+            "category_id"             => 1,// TODO いいのか確認する
+            "open_time"               => '',
+            "expire_time"             => '2020-10-10 12:00:00',
+            "close_time"              => null,
+            "admission_limit"         => '',
+            "freestyle_flag"          => '1',
+            "script_swf_flag"         => 1,
+            "ppv_permit_flag"         => 0,
+            "large_size_flag"         => 0,
+            "nicowari_flag"           => 0,
+            "public"                  => 0, // 表示制限あり
+            "mailpost_limit"          => 0,
+            "dic_nondisplay_flag"     => 0,
+            "nico_park_flag"          => 0,
+            "mobile_default_flag"     => 1,
+            "use_fms_flag"            => 0,
+            "company_viewname"        => $app['old_blog_name'],
+            "e_kokusei_flag"          => 0,
+            "e_kokusei_twitter_flag"  => 0,
+            "quick_live_flag"         => 0,
+            "user_video_display_flag" => 0,
+            "anime_flag"              => 0,
+            "official_live_flag"      => 0,
+            "type_transition_flag"    => 0,
+            "reprintable_docomo_flag" => 0,
+            "reprintable_yahoo_flag"  => 0,
+            "staff_name"              => $app['old_blog_name'],
+            "remarks"                 => $app['note'],
+            "cp_letter_ng_flag"       => 0,
+            "mobile_ng_docomo"        => 0,
+            "mobile_ng_au"            => 0,
+            "mobile_ng_softbank"      => 0,
+            "mobile_ng_willcom"       => 0,
+            "mobile_ng_apple"         => 0,
+            "mobile_ng_vita"          => 0,
+            "mobile_ng_other"         => 0,
+            "ichiba_flag"             => 1,
+            "allow_domain"            => 'jp',
+            "capacity_limit"          => 30,
+            "screen_name"             => self::getScreenName($app['old_blog_url'], date('YmdHi')),
+            "exclusion_flag"          => 0,
+            "book_flag"               => 0,
+            "blog_flag"               => 0,
+            "first_month_free_flag"   => 0,
+            "rss_available_flag"      => 0,
+            "news_available_flag"     => 0,
+            "image_capacity_limit"    => 1000,
+            "backnumber_free_flag"    => 0,
+            "shy_flag"                => 0,
+            "matome_blog_type"        => 2 // 2chまとめブログのみのブログ
+        );
+        $channelInfo = new channelInfo();
+        // 念のため、channel_id すでにDBに登録されていないか確認
+        $existData = $channelInfo->selectDatabaseData($channelId);
+        $isNewRecord = empty($existData);
+        if ($isNewRecord) {
+            $queryResult = $channelInfo->insertChannelData($channelData);
+            if ($queryResult !== true) {
+                throw new MyException('チャンネルの登録に失敗しました: ' . $queryResult);
+            }
+            // チャンネルの登録に成功した場合、サポートユーザを登録する
+            ChannelUtils::registerSupportAccount(intval($app['owner_id']), $channelId);
+            return $queryResult;
+        } else {
+            throw new MyException('チャンネルIDが重複しています');
+        }
+    }
+    
+    const SCREENNAME_MAX_LENGTH = 32;
+    /**
+     * URLと接尾辞から32文字以内のスクリーンネームを取得する。
+     * @param string $url
+     * @param string $suffix
+     */
+    public static function getScreenName($url, $suffix) {
+        $convertedUrl = self::convertUrlToScreenName($url);
+        // SCREENNAME_MAX_LENGTH以内に切り詰める
+        if (strlen($convertedUrl) > self::SCREENNAME_MAX_LENGTH - strlen($suffix)) {
+            $convertedUrl = substr($convertedUrl, 0, self::SCREENNAME_MAX_LENGTH - strlen($suffix));
+        }
+        $screenName = $convertedUrl.$suffix;
+        
+        if (self::isAvailableScreenName($screenName) === false) {
+            // もし使用不可能なスクリーンネームとなった場合、sha1によりランダムな文字列を作り、それをスクリーンネームにしてしまう
+            $screenName = substr(sha1($screenName), 0, self::SCREENNAME_MAX_LENGTH);
+        }
+        return $screenName;
+    }
+    
+    private static function isAvailableScreenName($screenName) {
+        $available_obj = ChannelScreenNameCheckModel::isScreenNameAvailable($postdata['screen_name'], true, true);
+        if($available_obj['isAvailable'] === '0') {
+            return false;
+        }
+        return true;
+    }
+    
+    /**
+     * URLをチャンネルスクリーンネームとして使う用の文字列に加工する
+     * @param string $url
+     * @return string
+     */
+    public static function convertUrlToScreenName($url) {
+        $matches = array();
+        // ライブドアパターン
+        if (preg_match('#blog\.livedoor\.jp/([a-zA-Z0-9_]+)#', $url, $matches)) {
+            return $matches[1];
+        }
+        // FC2パターン
+        if (preg_match('#([a-zA-Z_0-9]+)\.blog[0-9]+\.fc2\.com#', $url, $matches)) {
+            return $matches[1];
+        }
+        
+        //そのた。先頭のhttp及び記号を除去し、残った文字列を利用する。
+        $url = preg_replace('#^https?[^a-zA-Z_0-9]*#', '', $url);
+        return preg_replace('#[^a-zA-Z_0-9]+#', '_', $url);
+    }
+}
