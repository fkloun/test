+        'account_kana' => array(
+            'type' => 'input',
+            'isRequire' => array( 'value' => true, ),
+        ),
+
+    );
+
+    /*
+     * err_msg を指定しなかった場合のデフォルトのエラーメッセージ
+     */
+    protected $defaultErrorMsg = array(
+        'isRequire' => '入力してください。',
+    );
+
+
+    /*
+     * コンストラクタ・ファクトリ・getter/setter等
+     */
+    public function __construct( $params, $userInfo ) {
+        $this->setParams( $params );
+        $this->setUserInfo( $userInfo );
+    }
+
+    public function factory( $params, $userInfo ) {
+        return new self( $params, $userInfo );
+    }
+
+    public function setParams( $params ) {
+        $this->params = $params;
+        return $this;
+    }
+
+    public function getParams() {
+        return $this->filterByElements( $this->params );
+    }
+
+    public function getParam( $name ) {
+        $result = $this->filterByElements( $this->params );
+        return $result[$name];
+    }
+
+    public function getUserInfo() {
+        return $this->userInfo;
+    }
+
+    public function setUserInfo( $userInfo ) {
+        $this->userInfo = $userInfo;
+        return $this;
+    }
+
+    public function getElements() {
+        return $this->elements;
+    }
+
+    public function getElement( $name ) {
+
+        if( isset( $this->elements[$name] ) ) {
+            return $this->elements[$name];
+        } else {
+            return null;
+        }
+    }
+
+    public function getElementNames() {
+        return array_keys($this->elements);
+    }
+
+    public function filterByElements( $params ) {
+        $paramsTemp = array();
+        foreach( $params as $key => $val ) {
+//            $element = $this->getElement( $key );
+            if( isset( $this->elements[$key] ) ) {
+                $paramsTemp[$key] = $val;
+            }
+        }
+        return $paramsTemp;
+    }
+
+    public function getOptionalMailFields() {
+
+        return array (
+            '', // dummy
+            'optional_mail1',
+            'optional_mail2',
+            'optional_mail3',
+            'optional_mail4',
+            'optional_mail5',
+        );
+    }
+
+    /*
+     * 希望開設日のリストを返す
+     * 14～90日内の月曜日を返す
+     */
+    public function getPublishDates() {
+
+        $dateList[0] = array('id' => 0, 'name' => '選択してください');
+
+        date_default_timezone_set('Asia/Tokyo');    // タイムゾーンを指定しないと、サマータイムで１時間ズレるとかで無駄に悩む
+
+        $ymd = date( "Ymd" );
+        $timestamp = strtotime( $ymd );
+        $res = date( "w", $timestamp );
+
+        // 今日から起算して何日足すと次の月曜日になるかという値
+        if ($res === '0') {
+            $offset = 1;
+        } elseif ( $res === '1') { // 14日目の月曜日は通す
+            $offset = 0;
+        } else {
+            $offset = 8 - $res;
+        }
+        $timestamp += ((14 + $offset) * 86400); // 次の月曜日のタイムスタンプ
+        $rest = 90 - 14 - $offset; // 2週間以降最初の月曜日から、90日目までの日数
+
+        $count = 1;
+        while ( $rest >= 0) {
+            $dateList[$timestamp] = array('id' => $timestamp, 'name' => date('Y/m/d', $timestamp) );
+            $timestamp += (7 * 86400);
+            $rest -= 7;
+        }
+
+        return $dateList;
+    }
+
+    public function getPublishDate( $timestamp ) {
+
+        $publishDates = $this->getPublishDates();
+        return $publishDates[$timestamp];
+    }
+
+    public function getChToolLastAppliactionByOwnerId( $ownerId ) {
+
+        $params = array(
+            'owner_id' => $ownerId,
+        );
+
+        $result = ApplicationDao::factory()
+            ->getChToolLastAppliaction( $params )
+        ;
+        return $result;
+    }
+
+
+
+    /**
+     *
+     * カテゴリの一覧を得る
+     */
+    public function getCategoryAll( $isWithoutQuickLive = false ) {
+
+        $results = ApplicationDao::factory()
+            ->getCategoryAll()
+        ;
+
+        array_unshift( $results, array('id' => 0, 'name' => '選択してください') );
+
+        if( $isWithoutQuickLive ) {
+            // 簡単生放送（category_idが11）または映画(12)の場合、カテゴリリストから削除する
+            foreach ( $results as $key => $val ) {
+                if( isset( $val['category_id'] ) && ( $val['category_id'] == 11 || $val['category_id'] == 12 ) ) {
+                    unset( $results[$key] );
+                }
+            }
+        }
+
+        return $results;
+    }
+
+    /**
+     *
+     * カテゴリ名をカテゴリIDから得る
+     * @param integer $categoryId
+     */
+    public function getCategoryName( $categoryId ) {
+
+        return ApplicationDao::factory()
+            ->getCategoryName( $categoryId )
+        ;
+    }
+
+    /*
+     * バリデーション
+     */
+    public function validate() {
+
+        $paramsTemp = $this->params;
+//print_r($paramsTemp);
+
+        foreach( $this->elements as $key => $val ) {
+
+            // 入力必須のチェック
+            if ( isset($val['isRequire']['value']) && $val['isRequire']['value'] ) {
+//echo("require");
+                if ( empty($paramsTemp[$key]) && $paramsTemp[$key] !== '0' && $paramsTemp[$key] !== '' ) {
+                    // セットされていない
+//echo("no set !!!!!");
+                    $name = $key;
+                    $type = 'isRequire';
+                    $msg = $val[$type]['err_msg'];
+                    $this->setErrorMsg( $name, $type, $msg );
+                    $this->isError = true;
+                }
+
+            } else {
+//echo("no require");
+            }
+
+            $str = $paramsTemp[$key];
+
+            // 入力必須でない項目が空なら、バリデーションの対象としない
+            if( ( empty( $val['isRequire']['value'] ) || !$val['isRequire']['value'] ) && (empty($str) || trim($str) === '' ) ) {
+                continue;
+            }
+
+            switch ( $val['type']) {
+                case 'input':
+                    break;
+
+                case 'text':
+                    break;
+
+                case 'url':
+                    $pattern = '/^(?:http|https):\/\/[\w,.:;&=+*%$#!?@()~\'\/-]+$/';
+                    if ( !preg_match($pattern, $str )) {
+                        $name = $key;
+                        $type = 'invalid';
+                        $msg = $val[$type]['err_msg'];
+                        $this->setErrorMsg( $name, $type, $msg );
+                        $this->isError = true;
+                    }
+                    break;
+
+                case 'phone':
+                    $number = preg_replace('/[^\d]+/', '', $str );
+                    if (! ('0' === $number{0} && (10 == strlen($number) || 11 == strlen($number)) ) )  {
+                        $name = $key;
+                        $type = 'invalid';
+                        $msg = $val[$type]['err_msg'];
+                        $this->setErrorMsg( $name, $type, $msg );
+                        $this->isError = true;
+                    }
+                    break;
+                case 'radio':
+                    break;
+
+                default:
+                    $this->isError = true;
+            }
+        }
+
+        if ( $this->isError ) {
+            throw new MyException( MyException::ERROR_PARAM );
+        }
+
+        return $this;
+    }
+
+    /*
+     * 内部用 エラーメッセージをセットする
+     */
+    private function setErrorMsg( $name, $type, $msg = null ) {
+
+        if( isset( $msg ) ) {
+            $this->errorMsgs[$name][$type] = $msg;
+        } else {
+            $this->errorMsgs[$name][$type] = $this->defaultErrorMsg[$type];
+        }
+        return $this;
+    }
+}
+
