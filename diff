diff --git test/payment/model/MediaPlanApplicationRepositoryTest.php test/payment/model/MediaPlanApplicationRepositoryTest.php
new file mode 100755
index 0000000..0df5f42
--- /dev/null
+++ test/payment/model/MediaPlanApplicationRepositoryTest.php
@@ -0,0 +1,62 @@
+<?php
+require_once dirname(__FILE__).'/../PaymentTestBase.php';
+require_once CLASS_DIR.'/application/mediaplan/model/MediaPlanApplicationRepository.php';
+
+class MediaPlanApplicationRepositoryTest extends PaymentTestBase {
+    /** @test */
+    public function test_getByOwnerId() {
+        $params = array(
+            'id'         => 1,
+            'owner_id'   => 100,
+            'owner_mail' => 'hoge@hoge.com',
+            'apply_div'  => 'APPLYING'
+        );
+        
+        $dao = $this->getMock('MediaPlanApplicationDao', array('findByOwnerId'));
+        $dao->expects($this->once())
+            ->method('findByOwnerId')
+            ->with($this->equalTo(100))
+            ->will($this->returnValue($params));
+        
+        $repository = new MediaPlanApplicationRepository($dao);
+        $entity = $repository->getByOwnerId(100);
+
+        $this->assertSame($entity->isApplying(), true);
+        $this->assertSame($entity->getParams(), $params);
+    }
+    
+    /** @test */
+    public function test_getByOwnerId_missing() {
+        $dao = $this->getMock('MediaPlanApplicationDao', array('findByOwnerId'));
+        $dao->expects($this->once())
+            ->method('findByOwnerId')
+            ->with($this->equalTo(101))
+            ->will($this->returnValue(null));
+        
+        $repository = new MediaPlanApplicationRepository($dao);
+        $entity = $repository->getByOwnerId(101);
+
+        $this->assertSame($entity->isApplying(), false);
+        $this->assertSame($entity->getParams(), null);
+    }
+    
+    /** @test */
+    public function test_store() {
+        $dao = $this->getMock('MediaPlanApplicationDao', array('insert'));
+        $dao->expects($this->once())
+            ->method('insert')
+            ->with($this->equalTo(array(
+                'old_blog_url' => 'test.com',
+                ':owner_id'          => 200,
+                ':owner_mail'        => 'test@test.com',
+                ':ip_address'  => '0.0.0.1'
+            )))
+            ->will($this->returnValue(true));
+        
+        $repository = new MediaPlanApplicationRepository($dao);
+        $repository->store(
+            new MediaPlanApplicationEntity(array( 'old_blog_url' => 'test.com' )),
+            array('id'=>200, 'mail'=>'test@test.com'),
+            '0.0.0.1');
+    }
+}
\ No newline at end of file
diff --git test/payment/model/MediaPlanApplicationValidatorTest.php test/payment/model/MediaPlanApplicationValidatorTest.php
new file mode 100755
index 0000000..689f247
--- /dev/null
+++ test/payment/model/MediaPlanApplicationValidatorTest.php
@@ -0,0 +1,77 @@
+<?php
+require_once dirname(__FILE__).'/../PaymentTestBase.php';
+require_once CLASS_DIR.'/application/mediaplan/model/MediaPlanApplicationValidator.php';
+
+class MediaPlanApplicationValidatorTest extends PaymentTestBase {
+    private $userInfo;
+    /** @setup */
+    public function setUp() {
+        $this->userInfo = array(
+            'id' => 1
+        );
+    }
+    
+    /** @test */
+    public function test_hasError_noParams() {
+        $params = array();
+        $validator = new MediaPlanApplicationValidator($params, $this->userInfo);
+        
+        $this->assertSame($validator->hasError(), true);
+        $this->assertSame($validator->getErrorMessages(), array(
+              'old_blog_name'     => array( 'isRequire' => "入力してください。" ),
+              'old_blog_url'      => array( 'isRequire' => "入力してください。" ),
+              'old_blog_daily_pv' => array( 'isRequire' => "入力してください。" ),
+              'new_blog_name'     => array( 'isRequire' => "入力してください。" )
+            )
+        );
+    }
+    
+    /** @test */
+    public function test_hasError_invalid() {
+        $params = array(
+          'old_blog_url'      => 'URLってなに',
+          'new_blog_name'     => '超テストブログ',
+          'old_blog_name'     => 'テストブログ',
+          'old_blog_daily_pv' => '私の戦闘力は53万PVです',
+        );
+        $validator = new MediaPlanApplicationValidator($params, $this->userInfo);
+        
+        $this->assertSame($validator->hasError(), true);
+        $this->assertSame($validator->getErrorMessages(), array(
+              'old_blog_url'      => array( 'invalid' => "入力がURLではありません。" ),
+            )
+        );
+    }
+    
+    /** @test */
+    public function test_hasError_ok() {
+        $params = array(
+          'old_blog_url'      => 'http://test.blog.com',
+          'new_blog_name'     => '超テストブログ',
+          'old_blog_name'     => 'テストブログ',
+          'old_blog_daily_pv' => '私の戦闘力は53万PVです',
+        );
+        $validator = new MediaPlanApplicationValidator($params, $this->userInfo);
+        $this->assertSame($validator->hasError(), false);
+    }
+
+    /** @test */
+    public function test_getParams() {
+        $params = array(
+            'old_blog_url'      => 'http://test.blog.com',
+            'new_blog_name'     => '超テストブログ',
+            'old_blog_name'     => 'テストブログ',
+            'old_blog_daily_pv' => '私の戦闘力は53万PVです',
+            'note'              => 'ありません',
+            'kankeinai'         => 'ほげほげ'
+        );
+        $validator = new MediaPlanApplicationValidator($params, $this->userInfo);
+        $this->assertSame($validator->getParams(), array(
+            'old_blog_url'      => 'http://test.blog.com',
+            'new_blog_name'     => '超テストブログ',
+            'old_blog_name'     => 'テストブログ',
+            'old_blog_daily_pv' => '私の戦闘力は53万PVです',
+            'note'              => 'ありません',
+        ));
+    }
+}
\ No newline at end of file
