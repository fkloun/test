diff --git payment/pay/common/class/application/dao/ChannelEstablishDao.php payment/pay/common/class/application/dao/ChannelEstablishDao.php
new file mode 100755
index 0000000..7a06613
--- /dev/null
+++ payment/pay/common/class/application/dao/ChannelEstablishDao.php
@@ -0,0 +1,32 @@
+<?php
+require_once CLASS_DIR.'/PremiumDbAccess.php';
+/**
+ * channelestablishテーブルのデータアクセスオブジェクト
+ * @author manato_kato
+ */
+class ChannelEstablishDao extends PremiumDbAccess {
+    public function __construct() {}
+    
+    public static function factory() {
+        return new self();
+    }
+    
+
+    /**
+     * 引数のオーナーIDに紐づくエントリを1つ取得する
+     * @param int $ownerId チャンネルのオーナーID
+     * @return エントリが存在すればそのエントリ、なければnull
+     */
+    public function findByOwnerId($ownerId) {
+        $dbh = $this->getDBH( self::DB_CHANNEL_SLAVE );
+        $sql = <<<SQL
+            SELECT *
+            FROM channelestablish
+            WHERE
+                owner_id=:owner_id
+SQL;
+        $sth = $this->prepare( $dbh, $sql );
+        $sth->execute( array( ':owner_id' => $ownerId ));
+        return $sth->fetch(PDO::FETCH_ASSOC);
+    }
+}
